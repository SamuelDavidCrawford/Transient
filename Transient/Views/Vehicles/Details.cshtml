@model Transient.Models.Vehicle

@{
    ViewBag.Title = Model.Name;
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>@Model.Name</h2>

<ul>
    <li>Vehicle Type: @Model.VehicleType.Name</li>
    <li>Born: @Model.Year.ToShortDateString()</li>
    <li>Year Fleeted: @Model.YearAdded.ToShortDateString()</li>
    <li>Stock: @Model.NumberInStock</li>
</ul>